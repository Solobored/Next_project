{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "HTML"
  },
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    [
      "clsx\\$\\$\\(([^)]*)\\)\\$\\$",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    [
      "classnames\\$\\$\\(([^)]*)\\)\\$\\$",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ],
    [
      "cva\\$\\$\\(([^)]*)\\)\\$\\$",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ],
    [
      "cn\\$\\$\\(([^)]*)\\)\\$\\$",
      "(?:'|\"|`)([^']*)(?:'|\"|`)"
    ]
  ]
}
